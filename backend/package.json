{
  "name": "@bosancz/bosancz-interni-backend",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "dev": "nest start --watch",
    "premigrations": "npm run build",
    "migrations:run": "npm run premigrations && typeorm migration:run -d dist/data-source.js",
    "migrations:generate": "npm run premigrations && typeorm migration:generate -d dist/data-source.js",
    "migrations:revert": "npm run premigrations && typeorm migration:revert -d dist/data-source.js",
    "migrations:create": "npm run premigrations && typeorm migration:create",
    "cli": "node dist/main-cli",
    "deploy:production": "docker build -t docker.bosan.cz/interni-sekce-backend:latest . && docker push docker.bosan.cz/interni-sekce-backend:latest",
    "deploy:test": "docker build -t docker.bosan.cz/interni-sekce-backend:test . && docker push docker.bosan.cz/interni-sekce-backend:test"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "change-case": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^3.2.1",
    "mongoose": "^6.8.4",
    "nest-commander": "^3.4.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.11",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/inquirer": "^8.1.3",
    "@types/jest": "28.1.8",
    "@types/luxon": "^3.2.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.10",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
