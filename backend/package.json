{
  "name": "@bosancz/bosancz-interni-backend",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "node dist/main.js",
    "dev": "nest start --watch",
    "premigrations": "npm run build",
    "migrations:run": "npm run premigrations && typeorm migration:run -d dist/data-source.js",
    "migrations:generate": "npm run premigrations && typeorm migration:generate -d dist/data-source.js",
    "migrations:revert": "npm run premigrations && typeorm migration:revert -d dist/data-source.js",
    "migrations:create": "npm run premigrations && typeorm migration:create",
    "cli": "node dist/main-cli",
    "import": "node dist/main-import",
    "upgrade:check": "npx npm-check-updates",
    "upgrade": "npx npm-check-updates -u && npm install"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.2",
    "@nestjs/core": "^10.2.2",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/platform-express": "^10.2.2",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.9",
    "@nestjs/typeorm": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "change-case": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "googleapis": "^126.0.1",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.1",
    "luxon": "^3.4.2",
    "mime-types": "^2.1.35",
    "mongoose": "^7.4.5",
    "nest-commander": "^3.11.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.16",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.4",
    "@types/luxon": "^3.3.1",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.7",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "29.6.4",
    "prettier": "^3.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}