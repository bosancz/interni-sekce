{
  "name": "@bosancz/bosancz-interni-frontend",
  "description": "Bošán interní sekce",
  "private": true,
  "scripts": {
    "build": "ng build --configuration production",
    "dev": "ng serve",
    "dev:test-server": "ng serve --configuration=test-server",
    "deploy:production": "docker build -t docker.bosan.cz/interni-sekce-frontend:latest . && docker push docker.bosan.cz/interni-sekce-frontend:latest",
    "deploy:test": "docker build --build-arg=\"NG_CONFIGURATION=test\" -t docker.bosan.cz/interni-sekce-frontend:test . && docker push docker.bosan.cz/interni-sekce-frontend:test",
    "api:generate": "openapi-generator-cli generate -g typescript-axios -o src/app/api -i http://localhost:3000/api-json"
  },
  "dependencies": {
    "@angular/animations": "^16.1.4",
    "@angular/common": "^16.1.4",
    "@angular/compiler": "^16.1.4",
    "@angular/core": "^16.1.4",
    "@angular/forms": "^16.1.4",
    "@angular/platform-browser": "^16.1.4",
    "@angular/router": "^16.1.4",
    "@angular/service-worker": "^16.1.4",
    "@editorjs/editorjs": "^2.26.3",
    "@editorjs/header": "^2.7.0",
    "@editorjs/image": "^2.7.1",
    "@editorjs/list": "^1.8.0",
    "@editorjs/marker": "^1.3.0",
    "@ionic/angular": "^6.4.1",
    "@ngneat/until-destroy": "^9.2.2",
    "axios": "^1.3.5",
    "bootstrap": "^4.6.2",
    "chart.js": "^4.0.1",
    "chartjs-adapter-luxon": "^1.3.0",
    "czech-holidays": "^1.0.2",
    "docx": "^7.8.0",
    "file-saver": "^2.0.5",
    "inflected": "^2.1.0",
    "luxon": "^3.1.1",
    "ng2-charts": "^4.1.1",
    "pretty-bytes": "^6.0.0",
    "rxjs": "^6.6.6",
    "swiper": "^8.4.5",
    "zone.js": "^0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.4",
    "@angular/cli": "^16.1.4",
    "@angular/compiler-cli": "^16.1.4",
    "@angular/language-service": "^16.1.4",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/file-saver": "^2.0.5",
    "@types/google.accounts": "^0.0.7",
    "@types/inflected": "^1.1.29",
    "@types/luxon": "^3.1.0",
    "typescript": "~5.1.6"
  }
}